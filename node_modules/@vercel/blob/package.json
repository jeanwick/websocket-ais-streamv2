{
  "name": "@vercel/blob",
  "version": "0.1.4",
  "description": "Ultra-low latency data at the edge",
  "homepage": "https://vercel.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/blob"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": {
        "node": "./dist/index.cjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {
    "undici": "./dist/undici-browser.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*": [
      "prettier --ignore-unknown --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "undici": "5.21.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@changesets/cli": "2.26.1",
    "@edge-runtime/jest-environment": "2.0.8",
    "@edge-runtime/types": "2.0.8",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@vercel/style-guide": "4.0.2",
    "axios": "1.3.5",
    "eslint": "8.38.0",
    "got": "12.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "tsup": "6.7.0",
    "typescript": "4.9.5"
  },
  "packageManager": "pnpm@8.3.0",
  "engines": {
    "node": ">=16.14"
  },
  "scripts": {
    "build": "tsup && cp src/undici-browser.js dist/undici-browser.js",
    "changeset": "pnpm changeset:before && changeset && pnpm changeset:after || pnpm changeset:after",
    "changeset:after": "mv pnpm-workspace.yaml.backup pnpm-workspace.yaml",
    "changeset:before": "mv pnpm-workspace.yaml pnpm-workspace.yaml.backup",
    "dev": "cp src/undici-browser.js dist/undici-browser.js && tsup --watch --clean=false",
    "eslint-check": "eslint --max-warnings=0 .",
    "precommit": "lint-staged",
    "prettier-check": "prettier --check .",
    "release": "pnpm build && pnpm changeset:before && changeset publish && pnpm changeset:after",
    "test": "jest --env @edge-runtime/jest-environment .test.ts && jest --env node .test.ts",
    "type-check": "tsc --noEmit",
    "version-packages": "pnpm changeset:before && changeset version && pnpm changeset:after"
  }
}